<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
</head>
<body>

<header>
    <h1 class="h1 d-inline-block">Book</h1>
    <div class="logo" >
        <img src="{{ asset("build/images/index.png") }}">
    </div>
</header>

<div class="container">
    <div class="row">
        <div class="col-8">
            {% block body %}{% endblock %}
        </div>
        <div class="col-4">
            {% block right %}{% endblock %}
        </div>
    </div>
</div>

{% block javascripts %}
    {{ encore_entry_script_tags('app') }}
{% endblock %}

</body>
</html>
